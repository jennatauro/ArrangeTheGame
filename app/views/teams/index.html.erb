<h1>Listing teams</h1>
 
<table>
  <tr>
    <th>Name</th>
    <th>Text</th>
  </tr>
 
  <% @teams.each do |team| %>
    <tr>
      <td><%= team.name %></td>
      <td><%= team.text %></td>
    </tr>
  <% end %>
</table>

<%= link_to 'Back', teams_path %>
<%= link_to 'Register new team', new_team_path %>